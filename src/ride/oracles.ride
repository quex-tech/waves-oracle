{-# STDLIB_VERSION 7 #-}
{-# CONTENT_TYPE DAPP #-}
{-# SCRIPT_TYPE ACCOUNT #-}

@Callable(i)
func isInPool(pk: ByteVector, pool: ByteVector) = {
  let present =
    match getBoolean(this, toBase58String(pool) + ":" + toBase58String(pk)) {
      case b: Boolean => b
      case _          => false
    }

  ([], present)
}
